<?xml version="1.0" encoding="UTF-8"?>
<metadata>

	<configuration>
		<logFile>log/NewsdayCustomMetadata.log</logFile>
		<!-- logLevel allowed values (in ascending order, in terms of level of detail):
			OFF, SEVERE, WARNING, INFO, CONFIG, FINE, FINER, FINEST
		-->
		<logLevel>FINER</logLevel>
	</configuration>
	
	<!--
		#####################################
		SP Metadata 
		#####################################
	 -->	
	<sp>
		<metadataNames>
			<sectors>SECTOR</sectors>
			<industries>INDUSTRY</industries>
			<keywords>KEYWORDS</keywords>
			<sentiment>SENTIMENT</sentiment>
		</metadataNames>
		
		<pubConfiguration>
			<item levelMatch="^ND*">ND</item>
			<item levelMatch="^AM*">AM</item>
		</pubConfiguration>
			
		<desk>
			<item>ALL</item>
			<item pub="ND">LONG ISLAND</item>
