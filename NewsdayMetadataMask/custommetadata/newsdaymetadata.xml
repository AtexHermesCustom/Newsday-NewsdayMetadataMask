			<item pub="ND">OP-ED</item>
			<item pub="ND">SPORTS</item>
			<item pub="ND">TRAVEL</item>
			<item pub="AM">AMNY</item>
		</desk>
		<priority>
			<item>LOW</item>
			<item>MEDIUM</item>
			<item>HIGH</item>
		</priority>
		<storyType>
			<item>ARTICLE</item>
			<item>COLUMN</item>
			<item>EDITORIAL</item>
			<item>LETTER</item>
			<item>OPINION</item>
			<item>REVIEW</item>
			<item>OBITUARY</item>
			<item>CUSTOM</item>
		</storyType>
		<label insertEmptyItem="1">
			<item>EXCLUSIVE</item>
			<item>COVER STORY</item>
			<item>CENTERPIECE</item>
			<item>DEVELOPING</item>
			<item>TENTATIVE</item>
			<item>WATCHING</item>
		</label>
		<reporter>
			<item>News</item>
			<item>Business</item>
			<item>Property</item>
			<item>Personal Finance/ Investment</item>
			<item>Computer/IT</item>
			<item>Education</item>
			<item>Motoring</item>
			<item>Food/Culinary/Wine</item>
			<item>Fashion &amp; Beauty</item>
			<item>Health</item>
			<item>Shopping</item>
			<item>Entertainment/Music/Movies/Dance</item>
			<item>Sports</item>
			<item>Travel</item>
			<item>Hobbies-Craft/Photography</item>
			<item>Wedding/Parenting/Homes/Family</item>
		</reporter>
		<printSection>
			<item></item>
		</printSection>
		<printSequence>
			<item></item>
